<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.napoleonittask">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NapoleonITTask">
        <activity android:name="com.example.napoleonittask.MainMenu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".authorisation.AuthActivity" />
        <activity android:name=".AddOrDeletePos.DeleteAddActivity" />
        <activity android:name=".activities.ListActivity" />
        <activity android:name=".activities.ModSearchActivity" />
        <activity android:name=".activities.MoreInfoActivity" />
    </application>

</manifest>